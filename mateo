  <script type='text/javascript'>  //<![CDATA[
 const textos = [
      `!Ven a<br>celebrar!`,
      `<span id="texto1000">Celebremos el<br>cumpleaños de</span><br><span id="edadcito2">Tadeo</span>`,
      `<span id="edadcito">4 añitos</span><br><span id="texto2000"> de alegrías, aprendizaje<br>y mucho amor</span>`,
      `<span id="texto1000">Te invitamos<br>a ser parte<br>de este día<br>muy especial</span>`,
      `Cada recuerdo queda para siempre`,
      `Tendremos comida y música`,
      `No olvides traer tu mejor sonrisa`,
      `Disfrutaremos juntos este día`,
      `¡Habrá muchas sorpresas!`,
      `Te esperamos con cariño`
    ];

    const delays = [1, 1, 1, 1, 5, 6, 7, 8, 9, 10]; // segundos entre textos
    const animaciones = [
      'fadeIn1','fadeIn7','fadeIn7','fadeIn8','fadeIn5',
      'fadeIn6','fadeIn7','fadeIn8','fadeIn9','fadeIn10'
    ];

    textos.forEach((texto, i) => {
      const contenedor = document.getElementById(`mensaje${i + 1}`);
      const delayInicial = delays[i];
      const animacion = animaciones[i];
      const velocidadLetra = 0.11;

      const temp = document.createElement('div');
      temp.innerHTML = texto;

      setTimeout(() => {
        let delayActual = 0;

        function procesarNodo(nodo, padre) {
          if (nodo.nodeType === Node.TEXT_NODE) {
            const palabras = nodo.textContent.split(/(\s+)/);
            palabras.forEach(palabra => {
              const spanPalabra = document.createElement('span');
              spanPalabra.className = 'word';
              [...palabra].forEach(letra => {
                const span = document.createElement('span');
                span.textContent = letra;
                span.className = 'char';
                span.style.animationName = animacion;
                span.style.animationDuration = '1s';
                span.style.animationDelay = `${delayInicial + delayActual}s`;
                span.style.animationFillMode = 'forwards';
                delayActual += velocidadLetra;
                spanPalabra.appendChild(span);
              });
              padre.appendChild(spanPalabra);
            });
          } else if (nodo.nodeType === Node.ELEMENT_NODE) {
            if (nodo.tagName === 'BR') {
              padre.appendChild(document.createElement('br'));
            } else if (nodo.tagName === 'IMG') {
              const img = nodo.cloneNode();
              img.style.animation = "fadeInImage 1s ease forwards";
              img.style.animationDelay = `${delayInicial + delayActual}s`;
              delayActual += 0.8; // la imagen suma tiempo al delay global
              padre.appendChild(img);
            } else {
              const copia = nodo.cloneNode(false);
              padre.appendChild(copia);
              nodo.childNodes.forEach(hijo => procesarNodo(hijo, copia));
            }
          }
        }

        temp.childNodes.forEach(n => procesarNodo(n, contenedor));
      }, delayInicial * 1000);
    });
  //]]>
</script>
 
 
      
        
        
        
      </div>

      </div>

    
    
         
    
  </div> 
